{
  "name": "mintbase-bridge",
  "version": "0.0.7",
  "private": false,
  "license": "MIT",
  "keywords": [
    "ethereum",
    "NFT",
    "react",
    "store",
    "mintbase",
    "mint"
  ],
  "main": "dist/Embed/index.js",
  "module": "dist/Embed/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "PORT=3006 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prep": "rm -rf dist && tsc src/Embed/index.tsx --outDir dist --jsx react --esModuleInterop --skipLibCheck --allowSyntheticDefaultImports --resolveJsonModule --moduleResolution node --strict",
    "publish:npm": "SET NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src/Embed --out-dir dist --copy-files"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "qs": "^6.9.3",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.0",
    "styled-icons": "^10.2.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mintbase/mintbase-bridge.git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@types/qs": "^6.9.1",
    "@types/styled-components": "^5.1.0",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "babel": {
    "presets": [
      "@babel/react"
    ]
  }
}
